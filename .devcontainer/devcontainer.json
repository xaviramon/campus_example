{
    "name": "AVD Universal",
    "image": "ghcr.io/aristanetworks/avd/universal:python3.12-avd-v6.0.0-dev9",
    "onCreateCommand": "pip3 install -r requirements.txt",
    "containerEnv": {
        "ansible_vault_pass": "ansible_vault_pass"
    },
    "mounts": [
        "type=bind,source=${localEnv:HOME}/.ssh,target=/home/avd/.ssh,readonly"
    ]
}
